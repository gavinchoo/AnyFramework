<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ImageViewStyle">
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="main_tab_bottom">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">5dip</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">@color/tabhost_tab_tv_color_selector</item>
        <item name="android:gravity">center_horizontal|bottom</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:textSize">@dimen/dimen_title3</item>
        <item name="android:background">@drawable/tab_selector_checked_bg</item>
    </style>

    <style name="common_phone">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:hint">手机号</item>
        <item name="android:inputType">phone</item>
        <item name="android:maxLength">11</item>
        <item name="android:paddingLeft">10dip</item>
        <item name="android:paddingRight">10dip</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textCursorDrawable">@drawable/editvew_cursor_drawble</item>
        <item name="android:textSize">@dimen/dimen_text</item>
    </style>
    
    <style name="common_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:background">@drawable/common_checkbox_bg</item>
        <item name="android:button">@null</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="common_reviewRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/add_review_rating_bar</item>
        <item name="android:minHeight">24dip</item>
        <item name="android:maxHeight">24dip</item>
    </style>

    <style name="ImageView_RightArrow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">10dip</item>
        <item name="android:background">@drawable/common_right_arrow</item>
    </style>

    <style name="TextView_RightArrow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">start</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">10dip</item>
        <item name="android:drawablePadding">10dip</item>
        <item name="android:drawableRight">@drawable/common_right_arrow</item>
    </style>
    
    <style name="TextView_Horizontal_Line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
